[
  {
    "name": "pehapkari",
    "image": "${image}",
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "networkMode": "awsvpc",
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "pehapkari"
      }
    },
    "environment": [
      {
        "name": "EXAMPLE",
        "value": "development"
      }
    ]
  }
]
